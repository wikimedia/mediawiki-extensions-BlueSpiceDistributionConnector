			<bpmndi:BPMNShape id="CheckoutPage_2_di" bpmnElement="CheckoutPage_2">
				<dc:Bounds x="910" y="310" width="100" height="80"/>
			</bpmndi:BPMNShape>
		</bpmndi:BPMNPlane>
	</bpmndi:BPMNDiagram>
</bpmn:definitions>
