        <di:waypoint x="790" y="120" />
        <di:waypoint x="860" y="120" />
    </bpmndi:BPMNEdge>

    <bpmndi:BPMNEdge id="FromGatewayToApproveRevision_di" bpmnElement="FromGatewayToApproveRevision">
        <di:waypoint x="625" y="120" />
